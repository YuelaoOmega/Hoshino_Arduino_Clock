//RTC针脚
#define TIME_RST_PIN 3
#define TIME_DAT_PIN 4
#define TIME_CLK_PIN 5

//温度传感器针脚
#define TMP_PIN 2

//屏幕针脚
#define SCR_SDA_PIN 7
#define SCR_SCL_PIN 6

//音响针脚
#define AUD_CHANNEL_1_PIN 8
#define AUD_CHANNEL_2_PIN 9
#define AUD_CHANNEL_3_PIN 10
#define AUD_CHANNEL_4_PIN 11

//硬件对象构造
U8G2_SSD1309_128X64_NONAME0_F_SW_I2C screen(U8G2_R0, SCR_SCL_PIN, SCR_SDA_PIN);
DS1302 rtc(TIME_RST_PIN,TIME_DAT_PIN,TIME_CLK_PIN);
OneWire oneWire(TMP_PIN);
DallasTemperature temperatureSensor(&oneWire);

//========== 全局变量区 ==========
const int FRAME_DELAY = 32;  // 帧间隔 (ms)
const int DAY_FRAMES = 8;    // 日间动画总帧数
const int NIGHT_FRAMES = 4;  // 夜间动画总帧数
const int TRANS_FRAMES = 8;  // 过渡动画总帧数

//计数器和计时器
int currentFrameDay = 0;
int currentFrameNight = 0;
int currentFrameTrans = 0;
int currentFrameTransRe = 0;

unsigned long lastUpdateTemp = 0;
unsigned long lastUpdateDay = 0;
unsigned long lastUpdateNight = 0;
int int_Temp;


//判断变量
enum SystemState {
  STATE_DAY,
  STATE_NIGHT,
  STATE_TRANS_DAY_TO_NIGHT,
  STATE_TRANS_NIGHT_TO_DAY
};

SystemState systemState = STATE_DAY; // 新增状态枚举
bool temperatureRequested = false;

//音频初始化
void audio_setup(){
  //高电平触发播放
  pinMode(8, OUTPUT);
  pinMode(9, OUTPUT);
  pinMode(10, OUTPUT);
  pinMode(11, OUTPUT);
}


//初始化屏幕
void screen_prepare(void) {
  screen.setFontRefHeightExtendedText();
  screen.setDrawColor(1);
  screen.setFontPosTop();
  screen.setFontDirection(0);
  screen.setPowerSave(0);

}

//状态机
void considerState(){
  static bool firstrun = true;
  static bool lastNightState = false;

  Time now = rtc.time();
  bool isNight = (now.hr >= 21 || now.hr <= 8);
  
  if (firstrun){
    firstrun = false;
    lastNightState = isNight;
  }

  if (isNight != lastNightState) {

    reset_all_frame();
    systemState = isNight ? STATE_TRANS_NIGHT_TO_DAY : STATE_TRANS_DAY_TO_NIGHT;
    lastNightState = isNight;
    digitalWrite(AUD_CHANNEL_1_PIN, HIGH);
  } else {
    systemState = isNight ? STATE_NIGHT : STATE_DAY;
  }
}

//全部重置
void reset_all_frame(){
  currentFrameDay = 0;
  currentFrameNight = 0;
  currentFrameTrans = 0;
  currentFrameTransRe = 0;
}

//=========== 动画函数 ============
void draw_anime_night() {
  if (millis() - lastUpdateNight > FRAME_DELAY) {
    lastUpdateNight = millis();
    // 确认framesNight位图为64x64
    screen.drawXBMP(64, 13, 64, 51, framesNight[currentFrameNight]);
    if (++currentFrameNight >= 4) currentFrameNight = 0; // 假设有4帧
  }
}

void draw_anime_day() {
  if (millis() - lastUpdateDay > FRAME_DELAY) {
    lastUpdateDay = millis();
    // 确认framesDay位图为64x64
    screen.drawXBMP(64, 13, 64, 51, framesDay[currentFrameDay]);
    if (++currentFrameDay >= 8) currentFrameDay = 0; // 假设有8帧
  }
}

// ========== 过渡动画函数 ==========
void draw_transition() {
  static unsigned long lastUpdate = 0;
  if (millis() - lastUpdate > FRAME_DELAY) {
    lastUpdate = millis();

    screen.drawXBMP(64, 13, 64, 51, framesP[currentFrameTrans]);

    if (++currentFrameTrans >= TRANS_FRAMES) { // 严格匹配数组长度
      systemState = STATE_NIGHT;

      currentFrameTrans = 0;
    }
  }
}

void draw_transition_re() {
  static unsigned long lastUpdate = 0;
  if (millis() - lastUpdate > FRAME_DELAY) {
    lastUpdate = millis();

    screen.drawXBMP(64, 13, 64, 51, framesPre[currentFrameTransRe]);

    if (++currentFrameTransRe >= TRANS_FRAMES) { // 严格匹配数组长度
      systemState = STATE_DAY;
      currentFrameTransRe = 0;
    }
  }
}

//===================主程序

void setup() {
  //由于内存原因，第一次使用的时候先运行checkTime校准RTC即可
  //在另一个文件里
  //调试端口，完成开发后注释
  Serial.begin(9600);

  // 初始化U8g2
  screen.begin();          
  screen_prepare();
  
  
  //启动温度
  temperatureSensor.begin();
  //在这里请求第一次温度
  temperatureSensor.requestTemperatures();
  temperatureRequested = true;

  audio_setup();

  digitalWrite(AUD_CHANNEL_2_PIN, HIGH);
  delay(2000);



}

void loop() { 
  if (temperatureRequested) {
    if (millis() - lastUpdateTemp >= 1000) {
      int_Temp = temperatureSensor.getTempCByIndex(0);
      if (int_Temp >= 30){
        digitalWrite(AUD_CHANNEL_3_PIN, HIGH);
      }
          
      temperatureRequested = false; // 完成温度读取
      temperatureSensor.requestTemperatures();
      lastUpdateTemp = millis();
      temperatureRequested = true;

          
    }
  }

  considerState();
  screen.clearBuffer();
 

  Time now = rtc.time();
  char bufDate[20];
  char bufTime[20];
  char bufTemp[10];

  const char* dayNow = "Unknown";
  switch(now.day){
    case 7:
      dayNow = "Sunday";
      break; 
    case 1:
      dayNow = "Monday";
      break;
    case 2:
      dayNow = "Tuesday";
      break;
    case 3:
      dayNow = "Wednesday";
      break;
    case 4:
      dayNow = "Thursday";
      break;
    case 5:
      dayNow = "Friday";
      break;
    case 6:
      dayNow = "Saturday";
      break;
  }

  //写入日期时间温度
  snprintf(bufDate, sizeof(bufDate), "%04d-%02d-%02d",
            now.yr, now.mon, now.date);
  snprintf(bufTime, sizeof(bufTime), "%02d:%02d:%02d",
            now.hr, now.min, now.sec);
  snprintf(bufTemp, sizeof(bufTemp), "Temp:%02d",
            int_Temp);
  
  
  
  //渲染信息
  
  screen.setFont(u8g2_font_7x13_tf);
  screen.drawStr(0,5,bufTime);

  screen.setFont(u8g2_font_5x7_tf);
  screen.drawStr(0,18,bufDate);
  screen.drawStr(0,25,dayNow);
  screen.setFont(u8g2_font_7x13_tf);
  screen.drawStr(0,50,bufTemp);
  //=============================渲染动画

  switch(systemState) {
    case STATE_DAY:        
      draw_anime_day();
      break;
    case STATE_NIGHT:      
      draw_anime_night(); 
      break;

    case STATE_TRANS_DAY_TO_NIGHT: 
      draw_transition(); 
      break;
    case STATE_TRANS_NIGHT_TO_DAY: 
      draw_transition_re(); 
      break;
  }
  
  
  screen.sendBuffer();// 统一提交绘制
}



